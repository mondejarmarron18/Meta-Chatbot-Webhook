generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ticket {
  id                Int      @id @default(autoincrement())
  clientName        String
  clientEmail       String
  projectName       String
  projectStatus     String
  issuesAndConcerns String
  createdAt         DateTime @default(now())
}
